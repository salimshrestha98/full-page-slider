{
	"apiVersion": 2,
	"name": "full-page-slider/slide",
	"title": "Slide",
	"category": "layout",
	"icon": "media-document",
	"parent": ["full-page-slider/full-page-content-slider"],
	"attributes": {
	  "title": {
		"type": "string",
		"default": "Slide Title"
	  },
	  "footer": {
		"type": "string",
		"default": "Slide Footer"
	  },
	  "padding": {
		"type": "object",
		"default": {
			"top": 20,
			"right": 20,
			"bottom": 20,
			"left": 20,
			"unit": "px"
		}
	  },
	  "backgroundImage": {
		"type": "object",
		"default": {
			"id": 0,
			"url": "",
			"alt": ""
		}
	  },
		"backgroundSize": {
			"type": "string",
			"default": "cover"
		},
		"backgroundPosition": {
			"type": "string",
			"default": "center center"
		},
		"backgroundRepeat": {
			"type": "string",
			"default": "no-repeat"
		},
		"backgroundAttachment": {
			"type": "string",
			"default": "scroll"
		},
		"backgroundOverlay": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": [
		"full-page-slider/showTitle",
		"full-page-slider/titleAlignment",
		"full-page-slider/stickToBottom",
		"full-page-slider/padding"
	],
	"supports": {
		"html": false,
		"customClassName": false,
		"inserter": false,
		"reusable": false,
		"__experimentalHideToolbarControls": true
	},
	"editorScript": "file:./index.js",
	"style": "file:style-index.css"
  }
  